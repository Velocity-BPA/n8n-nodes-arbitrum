{
  "name": "n8n-nodes-arbitrum",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Arbitrum blockchain providing 14 resources and 111+ operations for L1-L2 bridging, DeFi, NFTs, smart contracts, and Stylus WASM support.",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "arbitrum",
    "ethereum",
    "layer2",
    "blockchain",
    "web3",
    "defi",
    "bridge",
    "stylus",
    "nft",
    "erc20",
    "smart-contracts",
    "retryable-tickets",
    "arbitrum-one",
    "arbitrum-nova"
  ],
  "license": "BUSL-1.1",
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-arbitrum",
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-arbitrum.git"
  },
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "main": "dist/nodes/Arbitrum/Arbitrum.node.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write 'nodes/**/*.ts' 'credentials/**/*.ts'",
    "lint": "eslint nodes credentials --ext .ts",
    "lint:fix": "eslint nodes credentials --ext .ts --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/integration",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ArbitrumRpc.credentials.js",
      "dist/credentials/Arbiscan.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Arbitrum/Arbitrum.node.js",
      "dist/nodes/Arbitrum/ArbitrumTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.20.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "ethers": "^6.9.0",
    "axios": "^1.6.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
